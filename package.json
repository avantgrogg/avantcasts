{
  "name": "avantcasts",
  "version": "0.0.1",
  "description": "A podcast app.",
  "main": "build/index.js",
  "scripts": {
    "start": "neutrino start --presets neutrino-preset-node && node build/index.js",
    "start-inspection": "neutrino start --presets neutrino-preset-node && node build/index.js --inspect",
    "build": "neutrino build --presets neutrino-preset-node",
    "test": "neutrino test --presets neutrino-preset-node neutrino-preset-mocha"
  },
  "keywords": [
    "koa",
    "koa2",
    "graphql",
    "react",
    "redux",
    "mocha"
  ],
  "author": "Douglas Grogg",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^3.17.0",
    "eslint-plugin-react": "^6.10.0",
    "koa": "^2.0.1",
    "neutrino": "^4.3.1",
    "neutrino-preset-mocha": "^4.2.0",
    "neutrino-preset-node": "^4.2.2",
    "neutrino-preset-react": "^4.2.3"
  },
  "repository": "git@github.com:avantgrogg/avantcasts.git",
  "dependencies": {
    "firebase-admin": "^4.1.3",
    "koa-better-router": "^2.1.1",
    "node-fetch": "^1.6.3"
  }
}
